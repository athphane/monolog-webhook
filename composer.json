{
    "name": "athphane/monolog-webhook",
    "description": "Monolog handler that sends messages to webhook endpoints",
    "type": "package",
    "autoload": {
        "psr-4": {
            "Athphane\\MonologWebhook\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Athphane\\MonologWebhook\\MonologWebhookServiceProvider"
            ]
        }
    },
    "require": {
        "ext-json": "*",
        "monolog/monolog": "^2.3",
        "guzzlehttp/guzzle": "6.5.5"
    }
}
